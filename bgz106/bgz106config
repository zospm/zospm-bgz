#!/bin/sh
# Need to perform product enablement 

id="5737-I22"
enablementText="PRODUCT OWNER('IBM CORP')               
        NAME('IBM Z OPEN DEV')              
        ID(${id})                    
        VERSION(*) RELEASE(*) MOD(*)    
        FEATURENAME('Dependency-Based-Build')
        STATE(ENABLED)"

out=$(whence zospmexternalfuncs >/dev/null)
if [ $? -eq 0 ]; then
	. zospmexternalfuncs
else    	
	echo "zospm tools need to be in your PATH" >&2
	exit 4
fi

swregistrar "bgz106" "enable" "${id}" "${enablementText}"
rc=$?
exit $rc
